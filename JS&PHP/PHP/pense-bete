class Enclos{
    constructor() {
        this._currentTimer = {
            hours: 0,
            min: 0,
            sec: 0
        }
    get action() {
        return this._action;
    }

    set action(value) {
        this._action = value;
    }
     set email(email) {
        if(/^(([^<>()[]\.,;:s@]+(.[^<>()[]\.,;:s@]+)*)|(.+))@(([[0-9]{1,3}.[0-9]{1,3}.[0-9]{1,3}.[0-9]	{1,3}])|(([a-zA-Z-0-9]+.)+[a-zA-Z]{2,}))$/.test(email)){
        this._email = email;
        } else {
            this._messageError = "format email invalide";
        }
    }
}
